body {
    background-color: #011a5c;
    font-family: Arial, Helvetica, sans-serif;
}

header {
    color: #FFFFFF;
    border: 1px solid;
    background:  #a90b2c;
    background-image: url(uaLogo.jpg),linear-gradient(#a90b2c,#FFFFFF);
    background-repeat: no-repeat;
    background-position: center;
    background-size: 200px 90px;
    margin-bottom: 20px;
    
}
header div, header a, header h1 {
    color: #FFFFFF;
    margin-top: 0;
    margin-left: 1px;
    padding-top: 0;
    padding-left: 10px;
    text-decoration: none;
}
header a:hover{
    color: blue;
}
header a:visited{
    color: blue;
}

nav {
    margin-bottom: 10px;
    font-size: 150%;
    text-align: center;
    border: 1px solid;
    background-color: #FFFFFF;
}
nav a {
    background-color: #FFFFFF;
    padding-left: 10px;
    padding-right: 10px;
}

h3{
    padding-left: 10px;
    text-decoration: underline;
    margin-bottom: 10px;
}

.indexBod{
    background-color: #FFFFFF;
}
.indexBod .mainHeader3, #indexBod .paraMain{
    background-color: #FFFFFF;
}
.indexBod p{
    margin-top: 0;
    margin-bottom: 10px;
    padding-left: 30px;
    padding-bottom: 10px;
}
video{
	display: block;
	margin: auto;
}

.productWrap{
    margin-top: 1%;
    padding-left: 7%;
    padding-right: 7%;
    padding-top: 2%;
    padding-bottom: 2%;
    overflow-x: scroll;
}

footer p, footer a{
    border: 1px solid;
    background-color: #FFFFFF;
}

.proImg{
    width: 225px;
    height: 225px;
}

#logo{
    padding-right: 10px;
    padding-top: 4px;
    height: 70px;
    width: 80px;
    float: right;
    z-index: 3;
    border-radius: 60%;
}
#external-link{
	margin: auto;
}

